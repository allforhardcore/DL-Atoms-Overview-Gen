{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["@figma/plugin-typings"],
    "lib": ["es2015", "dom"]
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
